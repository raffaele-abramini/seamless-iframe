{
  "name": "seamless-iframe",
  "version": "0.0.5",
  "description": "Seamless Iframe is a customisable library that renders HTML content in iframes as if it was part of your page.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "start": "npx parcel public/index.html",
    "start:e2eApp": "npx parcel public/e2e/index.html",
    "jest": "jest",
    "build": "rimraf ./dist && tsc",
    "cypress:run": "cypress run --record=false",
    "cypress": "cypress open",
    "lint": "npx eslint {src,cypress,public}/**/*.{j,t}s{,x} --no-error-on-unmatched-pattern"
  },
  "author": "Raffaele Abramini",
  "license": "ISC",
  "devDependencies": {
    "@types/dompurify": "^2.2.1",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "cypress": "^6.6.0",
    "dompurify": "^2.2.6",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "parcel": "^1.12.4",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raffaele-abramini/seamless-iframe.git"
  },
  "homepage": "https://github.com/raffaele-abramini/seamless-iframe#readme",
  "peerDependencies": {
    "react": ">= 16",
    "react-dom": ">= 16"
  },
  "dependencies": {},
  "jest": {
    "rootDir": "src",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$"
  },
  "resolutions": {
    "node-forge": "0.10.0"
  }
}
