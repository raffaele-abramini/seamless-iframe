{
  "name": "seamless-iframe",
  "version": "0.0.3-rc11",
  "description": "A helper to safely render html in iframe",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npx parcel public/index.html public/demo.html",
    "build": "npx rimraf ./docs && npx parcel build public/index.html public/demo.html --out-dir=docs && git add ./docs"
  },
  "author": "Raffaele Abramini",
  "license": "ISC",
  "devDependencies": {
    "@types/dompurify": "^2.2.1",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "dompurify": "^2.2.6",
    "jest": "^26.6.3",
    "parcel": "^1.12.4",
    "parcel-plugin-custom-dist-structure": "^1.1.16",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raffaele-abramini/seamless-iframe.git"
  },
  "homepage": "https://github.com/raffaele-abramini/seamless-iframe#readme",
  "peerDependencies": {
    "react": ">= 16",
    "react-dom": ">= 16"
  },
  "dependencies": {
    "seamless-iframe": "^0.0.5-rc4"
  },
  "jest": {
    "rootDir": "src",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$"
  },
  "customDistStructure": {
    "config": {
      "assets": [
        ".js",
        ".css",
        ".map",
        "!demo.html",
        "!index.html"
      ]
    }
  }
}
